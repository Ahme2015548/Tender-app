{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VERCEL_NODE_VERSION": "20.19.0"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}